---
import Layout from '../layouts/Layout.astro';
import HowItWorksSeccion from '../components/index/Secciones/HowItWorksSeccion.astro';
import UWIFISeccion from '../components/index/Secciones/UWIFISeccion.astro';
import ConectionSeccion from '../components/index/Secciones/ConectionSeccion.astro';



import {getPageData} from '../services/graphql.ts';
import * as HowItWorksData from '../data/HowItWorksQuerys.ts';



const data = await getPageData(HowItWorksData.GetHowItWorksSeccionFull());



---

<Layout title="How it works?">
	

	<HowItWorksSeccion strapiData={data}/> 
	<UWIFISeccion strapiData={data}/>
	<ConectionSeccion strapiData={data}/>

</Layout>




