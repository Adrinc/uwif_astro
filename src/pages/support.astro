---
import Layout from '../layouts/Layout.astro';
import SupportSeccion from '../components/support/Secciones/SupportSeccion.astro';
import ContactNumbers from '../components/support/Secciones/ContactNumbers.astro';
import CommonQuestionsSeccion from '../components/support/Secciones/CommonQuestionsSeccion.astro';
import Robot from '../components/global/Robot.astro';


import {getPageData} from '../services/graphql.ts';
import * as SupportQuerys from '../data/SupportQuerys.ts';



const data = await getPageData(SupportQuerys.GetSupportFull());

---



<Layout title="FAQ & SUPPORT">
	
<SupportSeccion strapiData={data}/>
<ContactNumbers strapiData={data}/>
<CommonQuestionsSeccion strapiData={data}/>
<Robot/>

</Layout>
